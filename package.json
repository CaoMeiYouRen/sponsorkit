{
  "name": "sponsorkit",
  "type": "module",
  "version": "16.5.0",
  "packageManager": "pnpm@10.14.0",
  "description": "Toolkit for generating sponsors images",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/sponsorkit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/sponsorkit.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/sponsorkit/issues"
  },
  "keywords": [
    "sponsors",
    "github-sponsors"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "sponsorkit": "./bin/sponsorkit.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "dev": "tsx src/cli.ts",
    "test": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish"
  },
  "dependencies": {
    "ansis": "^4.1.0",
    "cac": "^6.7.14",
    "consola": "^3.4.2",
    "dotenv": "^17.2.1",
    "ofetch": "^1.4.1",
    "sharp": "^0.34.3",
    "unconfig": "^7.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@antfu/ni": "^25.0.0",
    "@antfu/utils": "^9.2.0",
    "@fast-csv/parse": "^5.0.5",
    "@types/d3-hierarchy": "^3.1.7",
    "@types/node": "^24.3.0",
    "bumpp": "^10.2.3",
    "d3-hierarchy": "^3.1.2",
    "eslint": "^9.33.0",
    "jiti": "^2.5.1",
    "normalize-url": "^8.0.2",
    "p-limit": "^7.0.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "resolutions": {
    "sponsorkit": "workspace:*"
  }
}
